{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Foundry Server",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node",
      "outputCapture": "std",
      "localRoot": "C:\\Program Files\\FoundryVTT\\resources\\app\\public\\scripts\\",
      "remoteRoot": "\\localhost:3030\\scripts\\",
    },
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "pwa-chrome",
      "url": "http://localhost:3030",
      "webRoot": "${workspaceFolder}/module"
    },
    {
      "name": "Scratchpad",
      "program": "${workspaceFolder}/scratchpad.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
  ]
}